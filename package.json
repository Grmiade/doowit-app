{
  "name": "doowit-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "graphql:generate-types": "apollo client:codegen --globalTypesFile ./src/__generated__/globalTypes.ts --target typescript",
    "lint": "eslint './src/**'",
    "lint:fix": "eslint --fix './src/**'",
    "now-build": "react-scripts build",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.0.0",
    "@apollo/react-testing": "3.0.0",
    "@blueprintjs/core": "^3.18.0",
    "@blueprintjs/icons": "^3.10.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-debounce": "^2.1.0",
    "apollo-link-http": "^1.5.15",
    "apollo-link-retry": "^2.2.14",
    "apollo-link-ws": "^1.0.18",
    "apollo-utilities": "^1.3.2",
    "graphql": "^14.4.2",
    "normalize.css": "^8.0.1",
    "react": "^16.9.0-rc.0",
    "react-dom": "^16.9.0-rc.0",
    "styled-components": "^4.3.2",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.8",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.0",
    "@types/react": "^16.8.25",
    "@types/react-dom": "^16.8.5",
    "@types/styled-components": "^4.1.18",
    "apollo": "^2.17.2",
    "faker": "^4.1.0",
    "graphql.macro": "^1.4.2",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "now": "^16.1.1",
    "prettier": "^1.18.2",
    "react-scripts": "3.0.1",
    "typescript": "^3.5.3",
    "typescript-styled-plugin": "^0.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{json,scss,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions"
    ]
  }
}
