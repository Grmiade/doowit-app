{
  "name": "doowit-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "graphql:generate-types": "apollo client:codegen --globalTypesFile ./src/__generated__/globalTypes.ts --target typescript",
    "lint": "eslint './src/**'",
    "lint:fix": "eslint --fix './src/**'",
    "now-build": "react-scripts build",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@apollo/react-hooks": "0.1.0-beta.7",
    "@blueprintjs/core": "^3.16.0",
    "@blueprintjs/icons": "^3.9.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-debounce": "^2.1.0",
    "apollo-link-http": "^1.5.15",
    "apollo-link-retry": "^2.2.14",
    "apollo-link-ws": "^1.0.18",
    "apollo-utilities": "^1.3.2",
    "graphql": "^14.3.1",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.1",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@testing-library/react": "^8.0.1",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.2.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.16",
    "apollo": "^2.13.1",
    "faker": "^4.1.0",
    "graphql.macro": "^1.4.2",
    "husky": "^2.4.1",
    "jest-dom": "^3.5.0",
    "lint-staged": "^8.2.1",
    "now": "^15.5.0",
    "prettier": "^1.18.2",
    "react-scripts": "3.0.1",
    "typescript": "^3.5.2",
    "typescript-styled-plugin": "^0.14.0"
  },
  "resolutions": {
    "caniuse-lite": "1.0.30000974"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{json,scss,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions"
    ]
  }
}
