{
  "name": "doowit-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "graphql:generate-types": "apollo client:codegen --customScalarsPrefix=GraphQL --globalTypesFile ./src/__generated__/globalTypes.ts --passthroughCustomScalars --target typescript",
    "lint": "eslint --quiet './src/**'",
    "now-build": "react-scripts build",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@blueprintjs/core": "3.14.1",
    "@blueprintjs/icons": "3.6.0",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.9",
    "apollo-link-debounce": "2.1.0",
    "apollo-link-http": "1.5.12",
    "apollo-link-retry": "2.2.11",
    "apollo-link-ws": "1.0.15",
    "apollo-utilities": "1.2.1",
    "graphql": "14.1.1",
    "react": "16.8.4",
    "react-apollo": "2.5.2",
    "react-dom": "16.8.4",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@types/faker": "4.1.5",
    "@types/graphql": "14.0.7",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "@types/styled-components": "4.1.12",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "@typescript-eslint/parser": "1.4.2",
    "apollo": "2.6.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-react-hooks": "1.5.0",
    "faker": "4.1.0",
    "graphql.macro": "1.3.5",
    "husky": "1.3.1",
    "jest-dom": "3.1.3",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "react-scripts": "2.1.8",
    "react-testing-library": "6.0.0",
    "typescript": "3.3.3333",
    "typescript-styled-plugin": "0.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{json,scss,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
