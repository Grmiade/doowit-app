{
  "name": "doowit-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "parcel ./src/index.html --open",
    "build": "parcel build ./src/index.html",
    "graphql:generate-types": "apollo codegen:generate --addTypename --passthroughCustomScalars --globalTypesFile ./src/__generated__/globalTypes.ts --target typescript",
    "lint": "tslint --config tslint.json --project tsconfig.json './src/**/*.{ts,tsx}'",
    "precommit": "lint-staged"
  },
  "alias": {
    "components": "./src/components/"
  },
  "dependencies": {
    "@blueprintjs/core": "3.10.0",
    "@blueprintjs/icons": "3.4.0",
    "apollo-cache-inmemory": "1.3.12",
    "apollo-client": "2.4.8",
    "apollo-link": "1.2.6",
    "apollo-link-http": "1.5.9",
    "apollo-link-ws": "1.0.12",
    "apollo-utilities": "1.0.27",
    "components": "0.1.0",
    "formik": "1.4.1",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "react": "16.7.0",
    "react-apollo": "2.3.3",
    "react-dom": "16.7.0",
    "styled-components": "4.1.3",
    "subscriptions-transport-ws": "0.9.15"
  },
  "devDependencies": {
    "@types/node": "10.12.18",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/styled-components": "^4.1.4",
    "apollo": "2.1.8",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "node-sass": "4.11.0",
    "parcel-bundler": "1.11.0",
    "prettier": "1.15.3",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-react": "3.6.0",
    "typescript": "3.2.2"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,scss}": [
      "prettier --write",
      "git add"
    ]
  }
}
