{
  "name": "doowit-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "parcel ./src/index.html --open",
    "build": "parcel build ./src/index.html",
    "graphql:generate-types": "apollo client:codegen --addTypename --customScalarsPrefix=GraphQL --globalTypesFile ./src/__generated__/globalTypes.ts --passthroughCustomScalars --target typescript",
    "lint": "tslint --config tslint.json --project tsconfig.json './src/**/*.{ts,tsx}'"
  },
  "alias": {
    "components": "./src/components/"
  },
  "dependencies": {
    "@blueprintjs/core": "3.12.0",
    "@blueprintjs/icons": "3.5.1",
    "apollo-cache-inmemory": "1.4.2",
    "apollo-client": "2.4.12",
    "apollo-link": "1.2.8",
    "apollo-link-debounce": "2.1.0",
    "apollo-link-http": "1.5.11",
    "apollo-link-retry": "2.2.10",
    "apollo-link-ws": "1.0.14",
    "apollo-utilities": "1.1.2",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.1",
    "react": "16.8.1",
    "react-apollo": "2.4.1",
    "react-dom": "16.8.1",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@types/graphql": "14.0.5",
    "@types/node": "10.12.21",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/styled-components": "4.1.8",
    "apollo": "2.4.4",
    "husky": "1.3.1",
    "lint-staged": "8.1.3",
    "node-sass": "4.11.0",
    "parcel-bundler": "1.11.0",
    "prettier": "1.16.4",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,scss,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
