{
  "name": "doowit-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "graphql:generate-types": "apollo client:codegen --addTypename --customScalarsPrefix=GraphQL --globalTypesFile ./src/__generated__/globalTypes.ts --passthroughCustomScalars --target typescript",
    "lint": "tslint --config tslint.json --project tsconfig.json './src/**/*.{ts,tsx}'",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@blueprintjs/core": "3.13.0",
    "@blueprintjs/icons": "3.6.0",
    "apollo-cache-inmemory": "1.4.3",
    "apollo-client": "2.4.13",
    "apollo-link": "1.2.8",
    "apollo-link-debounce": "2.1.0",
    "apollo-link-http": "1.5.11",
    "apollo-link-retry": "2.2.10",
    "apollo-link-ws": "1.0.14",
    "apollo-utilities": "1.1.3",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "react": "16.8.2",
    "react-apollo": "2.4.1",
    "react-dom": "16.8.2",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@types/graphql": "14.0.7",
    "@types/node": "11.9.4",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "@types/styled-components": "4.1.10",
    "apollo": "2.5.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "react-scripts": "2.1.5",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,scss,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
